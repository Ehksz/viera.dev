{
  "extends": "tsconfig/next",
  "compilerOptions": {
    "rootDir": "./",
    "paths": {
      "#hooks": [
        "./src/hooks"
      ],
      "#hooks/*": [
        "./src/hooks/*"
      ],
      "#utils": [
        "./src/utils"
      ],
      "#utils/*": [
        "./src/utils/*"
      ],
      "#components": [
        "./src/components"
      ],
      "#components/*": [
        "./src/components/*"
      ],
      "#redux": [
        "./src/redux"
      ],
      "#redux/*": [
        "./src/redux/*"
      ],
      "#config": [
        "./src/config"
      ],
      "#config/*": [
        "./src/config/*"
      ],
      "#context": [
        "./src/context"
      ],
      "#context/*": [
        "./src/context/*"
      ],
      "#hoc": [
        "./src/hoc"
      ],
      "#hoc/*": [
        "./src/hoc/*"
      ],
      "#services": [
        "./src/services"
      ],
      "#services/*": [
        "./src/services/*"
      ]
    },
    "moduleResolution": "node"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "../../packages/ui/components/PhoneInput.tsx",
    "next.config.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
